@model IEnumerable<OnlineCarBikeRentalSystem.Models.ViewModel.PackageViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
<br />
<button type="button" class="btn btn-info" style="margin-left:30px;"><i class="fas fa-plus"></i>@Html.ActionLink("Add New Package", "AddPackage")</button>
<table class="table table-striped table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PackageId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PackageHeading)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ListPackage)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PackageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PackageHeading)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ListPackage)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditPackage", new { id = item.PackageId }, new { @class = "btn btn-info" })|
                @Html.ActionLink("Delete", "Delete", new { id = item.PackageId }, new { @class = "btn btn-danger" })
            </td>
        </tr>
    }

</table>
<br />
<br />
